RUN apk add curl libarchive-tools
RUN curl -OL https://downloads.sourceforge.net/project/samtools/samtools/1.2/samtools-1.2.tar.bz2
RUN bsdtar xf samtools-1.2.tar.bz2

FROM alpine:3.9 AS buildenv
RUN apk add gcc make libc-dev ncurses-dev bzip2-dev zlib-dev curl-dev curl
COPY --from=download /samtools-1.2 /samtools-1.2
WORKDIR /samtools-1.2

RUN make -j4
RUN install -D -t /dest//usr/bin samtools


